{
  "diagnostic.virtualText": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "•",
  "diagnostic.hintSign": "•",
  "diagnostic.messageDelay": 200,
  "diagnostic.messageLevel": "error",

  "intelephense.diagnostics.enable": false,
  "intelephense.format.enable": false,
  "intelephense.telemetry.enabled": false,

  "prettier.formatterPriority": 1,

  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",

  "vetur.ignoreProjectWarning": true,

  "rust-analyzer.updates.prompt": false,
  "rust-analyzer.inlayHints.typeHints.enable": true,
  "rust-analyzer.inlayHints.renderColons": true,
  // "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.inlayHints.chainingHints.enable": true,
  "rust-analyzer.inlayHints.closingBraceHints.enable": true,
  // "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  // "rust-analyzer.inlayHints.parameterHints.enable": true,
  // "rust-analyzer.inlayHints.reborrowHints.enable": "always",
  // "rust-analyzer.inlayHints.typeHints.hideClosureInitialization": true,
  "rust-analyzer.checkOnSave.command": "clippy",

  "coc.preferences.formatOnSave": true,
  // "coc.preferences.formatOnSave": [
  //   "css",
  //   "rust",
  //   "javascript",
  //   "typescript",
  //   "typescriptreact",
  //   "javascriptreact",
  //   "vue"
  // ],


  "prettier.requireConfig": true,

  "eslint.autoFixOnSave": false,
  "eslint.format.enable": true,

  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  }
}
